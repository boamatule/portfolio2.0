{
	"name": "portfolio2.0",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"jest": {
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		]
	},
	"scripts": {
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"remove-unused": "node -e \"const depcheck = require('depcheck'); depcheck(process.cwd(), {}, unused => { console.log('Unused dependencies: ', unused.dependencies); if (unused.dependencies.length) { console.log('Removing unused dependencies...'); const { execSync } = require('child_process'); unused.dependencies.forEach(dep => { execSync('yarn remove ' + dep); console.log('Removed:', dep); }); } else { console.log('No unused dependencies found.'); } })\"",
		"lint": "eslint --fix -- .",
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"test": "craco test --env=jsdom",
		"test:coverage": "npm test -- --coverage",
		"test:badges": "npm run test:coverage && jest-coverage-badges",
		"eject": "react-scripts eject"
	},
	"dependencies": {
		"@cfaester/enzyme-adapter-react-18": "^0.7.0",
		"@craco/craco": "^7.1.0",
