		"react-dom": "^18.2.0",
		"react-icons": "^4.11.0",
		"react-lazy-load-image-component": "^1.6.0",
		"react-router-dom": "^6.14.2",
		"react-scripts": "^5.0.1",
		"react-scroll": "^1.8.9",
